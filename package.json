    "prettier": "^2.0.5"
  },
  "dependencies": {
    "lodash.isplainobject": "^4.0.6"
  },
  "jest": {
    "testMatch": ["**/test/**/*.js"]
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
